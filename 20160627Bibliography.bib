	Journal = {Science},
	M3 = {10.1126/science.1148861},
	Month = {01},
	Number = {5859},
	Pages = {91--93},
	Title = {Polymorphic Y Chromosomes Harbor Cryptic Variation with Manifold Functional Consequences},
	Ty = {JOUR},
