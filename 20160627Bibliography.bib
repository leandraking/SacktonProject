	Journal = {Genetics},
	Month = {08},
	Number = {4},
	Pages = {793--800},
	Title = {Position Effect Variegation in Drosophila Melanogaster: Relationship between Suppression Effect and the Amount of Y Chromosome},
	Ty = {JOUR},
	U1 = {1224793{$[$}PII{$]$}; 2503420{$[$}pmid{$]$}; 2503420{$[$}pmid{$]$}},
