@article{Lemos:2010aa,
	Author = {Lemos, Bernardo and Branco, Alan T. and Hartl, Daniel L.},
	Date = {2010/09/07},
	Date-Added = {2016-06-28 14:58:46 +0000},
	Date-Modified = {2016-06-28 18:39:37 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	M3 = {10.1073/pnas.1010383107},
	Month = {09},
	Number = {36},
	Pages = {15826--15831},
	Title = {Epigenetic effects of polymorphic Y chromosomes modulate chromatin components, immune response, and sexual conflict},
	Ty = {JOUR},
	Volume = {107},
	Year = {2010},
	Bdsk-Url-1 = {http://www.pnas.org/content/107/36/15826.abstract%20N2%20-%20Genetic%20conflicts%20between%20sexes%20and%20generations%20provide%20a%20foundation%20for%20understanding%20the%20functional%20evolution%20of%20sex%20chromosomes%20and%20sexually%20dimorphic%20phenotypes.%20Y%20chromosomes%20of%20Drosophila%20contain%20multi-megabase%20stretches%20of%20satellite%20DNA%20repeats%20and%20a%20handful%20of%20protein-coding%20genes%20that%20are%20monomorphic%20within%20species.%20Nevertheless,%20polymorphic%20variation%20in%20heterochromatic%20Y%20chromosomes%20of%20Drosophila%20result%20in%20genome-wide%20gene%20expression%20variation.%20Here%20we%20show%20that%20such%20naturally%20occurring%20Y-linked%20regulatory%20variation%20(YRV)%20can%20be%20detected%20in%20somatic%20tissues%20and%20contributes%20to%20the%20epigenetic%20balance%20of%20heterochromatin/euchromatin%20at%20three%20distinct%20loci%20showing%20position-effect%20variegation%20(PEV).%20Moreover,%20polymorphic%20Y%20chromosomes%20differentially%20affect%20the%20expression%20of%20thousands%20of%20genes%20in%20XXY%20female%20genotypes%20in%20which%20Y-linked%20protein-coding%20genes%20are%20not%20transcribed.%20The%20data%20show%20a%20disproportionate%20influence%20of%20YRV%20on%20the%20variable%20expression%20of%20genes%20whose%20protein%20products%20localize%20to%20the%20nucleus,%20have%20nucleic-acid%20binding%20activity,%20and%20are%20involved%20in%20transcription,%20chromosome%20organization,%20and%20chromatin%20assembly.%20These%20include%20key%20components%20such%20as%20HP1,%20Trithorax-like%20(GAGA%20factor),%20Su(var)3%E2%80%939,%20Brahma,%20MCM2,%20ORC2,%20and%20inner%20centromere%20protein.%20Furthermore,%20mitochondria-related%20genes,%20immune%20response%20genes,%20and%20transposable%20elements%20are%20also%20disproportionally%20affected%20by%20Y%20chromosome%20polymorphism.%20These%20functional%20clusterings%20may%20arise%20as%20a%20consequence%20of%20the%20involvement%20of%20Y-linked%20heterochromatin%20in%20the%20origin%20and%20resolution%20of%20genetic%20conflicts%20between%20males%20and%20females.%20Taken%20together,%20our%20results%20indicate%20that%20Y%20chromosome%20heterochromatin%20serves%20as%20a%20major%20source%20of%20epigenetic%20variation%20in%20natural%20populations%20that%20interacts%20with%20chromatin%20components%20to%20modulate%20the%20expression%20of%20biologically%20relevant%20phenotypic%20variation.}}

@article{Reeves:2001aa,
	Annote = {The mammalian HMGI/Y (HMGA) non-histone proteins participate in a wide variety of cellular processes including regulation of inducible gene transcription, integration of retroviruses into chromosomes and the induction of neoplastic transformation and promotion of metastatic progression of cancer cells. Recent advances have contributed greatly to our understanding of how the HMGI/Y proteins participate in the molecular mechanisms underlying these biological events. All members of the HMGI/Y family of `high mobility group'proteins are characterized by the presence of multiple copies of a conserved DNA-binding peptide motif called the `AT hook'that preferentially binds to the narrow minor groove of stretches of AT-rich sequence. The mammalian HMGI/Y proteins have little, if any, secondary structure in solution but assume distinct conformations when bound to substrates such as DNA or other proteins. Their intrinsic flexibility allows the HMGI/Y proteins to participate in specific protein-DNA and protein-protein interactions that induce both structural changes in chromatin substrates and the formation of stereospecific complexes called `enhanceosomes'on the promoter/enhancer regions of genes whose transcription they regulate. The formation of such regulatory complexes is characterized by reciprocal inductions of conformational changes in both the HMGI/Y proteins themselves and in their interacting substrates. It may well be that the inherent flexibility of the HMGI/Y proteins, combined with their ability to undergo reversible disordered-to-ordered structural transitions, has been a significant factor in the evolutionary selection of these proteins for their functional role(s) in cells.},
	Author = {Reeves, Raymond and Beckerbauer, Lois},
