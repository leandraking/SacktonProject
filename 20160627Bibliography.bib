	Number = {8},
	Pages = {3697--3702},
	Publisher = {American Chemical Society},
	Title = {Cytosine DNA Methylation Is Found in Drosophila melanogaster but Absent in Saccharomyces cerevisiae, Schizosaccharomyces pombe, and Other Yeast Species},
	Ty = {JOUR},
	U1 = {24640988{$[$}pmid{$]$}},
	Url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC4006885/},
	Volume = {86},
